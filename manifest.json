{
  "name": "LessNoise",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "LessNoise is a filter for Twitter's timeline",
  "update_url": "https://raw.github.com/cakebaker/lessnoise/master/update.xml",
  "converted_from_user_script": true,
  "content_scripts": [{
    "js": ["lib/jquery-1.8.3.min.js", 
           "lessnoise-for-chrome.user.js"],
    "css": ["css/lessnoise.css"],
    "matches": ["https://twitter.com/", "https://twitter.com/#"]
  }],
  "permissions": ["http://*/", "https://*/"]
}
